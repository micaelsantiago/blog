#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/micael/www/blog/node_modules/.pnpm/ts-jest@29.1.2_@babel+core@7.24.4_@jest+types@29.6.3_babel-jest@29.7.0_@babel+core@7.24.4__je_nv42vtwr43hjdyf4r3mh4ag3ne/node_modules/ts-jest/node_modules:/home/micael/www/blog/node_modules/.pnpm/ts-jest@29.1.2_@babel+core@7.24.4_@jest+types@29.6.3_babel-jest@29.7.0_@babel+core@7.24.4__je_nv42vtwr43hjdyf4r3mh4ag3ne/node_modules:/home/micael/www/blog/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/micael/www/blog/node_modules/.pnpm/ts-jest@29.1.2_@babel+core@7.24.4_@jest+types@29.6.3_babel-jest@29.7.0_@babel+core@7.24.4__je_nv42vtwr43hjdyf4r3mh4ag3ne/node_modules/ts-jest/node_modules:/home/micael/www/blog/node_modules/.pnpm/ts-jest@29.1.2_@babel+core@7.24.4_@jest+types@29.6.3_babel-jest@29.7.0_@babel+core@7.24.4__je_nv42vtwr43hjdyf4r3mh4ag3ne/node_modules:/home/micael/www/blog/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ts-jest/cli.js" "$@"
else
  exec node  "$basedir/../ts-jest/cli.js" "$@"
fi
